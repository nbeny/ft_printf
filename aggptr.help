#include <stdarg.h>
#include <stdio.h>

static int	ft_print_num(int num, ...)
{
	int	sum;
	va_list	argptr;
	int	i;

	sum = 0;
	i = 0;
	va_start(argptr, num);
	while (i < num)
	{
		sum += va_arg(argptr, int);
		i++;
	}
	va_end(argptr);
	return (sum);
}

static void	ft_print_str(int num, ...)
{
	int	i;
	char	*ptr;
	va_list	argptr;

	i = 0;
	va_start(argptr, num);
	while (i < num)
	{
		ptr = va_arg(argptr, char *);
		printf("ptr = %s\n", ptr);
		i++;
	}
	va_end(argptr);
}

